{
  "name": "mcp-query-assistant",
  "version": "0.1.1",
  "description": "A Model Context Protocol server that helps generate queries using semantic search over training examples",
  "type": "module",
  "main": "build/index.js",
  "scripts": {
    "build": "tsc && node -e \"require('fs').chmodSync('build/index.js', '755')\"",
    "prepare": "npm run build",
    "dev": "tsc --watch",
    "start:with-key": "node build/index.js --openai-key YOUR_OPENAI_API_KEY_HERE"
  },
  "keywords": [
    "mcp",
    "cypher",
    "sparql",
    "semantic-search",
    "query-assistant",
    "neo4j",
    "vector-search",
    "query-generation"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.0.0",
    "openai": "^4.0.0",
    "hnswlib-node": "^3.0.0",
    "fs-extra": "^11.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/fs-extra": "^11.0.0",
    "typescript": "^5.0.0"
  }
}
